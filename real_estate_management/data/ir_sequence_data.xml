<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Property Reference Sequence -->
        <record id="seq_real_estate_property" model="ir.sequence">
            <field name="name">Property Reference</field>
            <field name="code">real.estate.property.code</field>
            <field name="prefix">REF</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Contract Number Sequence -->
        <record id="seq_real_estate_contract" model="ir.sequence">
            <field name="name">Contract Number</field>
            <field name="code">real.estate.contract.code</field>
            <field name="prefix">CTR/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Maintenance Request Sequence -->
        <record id="seq_real_estate_maintenance" model="ir.sequence">
            <field name="name">Maintenance Request</field>
            <field name="code">real.estate.maintenance.code</field>
            <field name="prefix">MNT/%(year)s/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>
